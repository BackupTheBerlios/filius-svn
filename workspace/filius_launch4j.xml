<launch4jConfig>
  <headerType>gui</headerType>
  <outfile>Filius.exe</outfile>
  <jar>filius.jar</jar>
  <dontWrapJar>false</dontWrapJar>
  <errTitle>FILIUS - Error</errTitle>
  <downloadUrl>http://java.com/download</downloadUrl>
  <supportUrl></supportUrl>
  <cmdLine></cmdLine>
  <chdir>.</chdir>
  <icon>gfx/filius32.ico</icon>
  <classPath>
    <mainClass>filius/Main</mainClass>
    <cp>lib/htmlparser.jar</cp>
    <cp>lib/tools.jar</cp>
  </classPath>
  <jre>
    <!-- Specify one of the following or both -->
    <!-- <path></path> -->
    <minVersion>1.6.0</minVersion>
  </jre>
  <versionInfo>
    <fileVersion>1.3.1.4</fileVersion>
    <txtFileVersion>1.3.1.4</txtFileVersion>
    <fileDescription>Filius Network Simulator</fileDescription>
    <copyright>GPL</copyright>
    <productVersion>1.3.1.4</productVersion>
    <txtProductVersion>1.3.1.4</txtProductVersion>
    <productName>Filius Network Simulator</productName>
    <companyName>Uni Siegen</companyName>
    <internalName>Filius</internalName>
    <originalFilename>Filius.exe</originalFilename>
  </versionInfo>
  <messages>
    <startupErr>Filius startup</startupErr>
    <bundledJreErr>Error: bundled JRE not found</bundledJreErr>
    <jreVersionErr>Error: JRE Version Error</jreVersionErr>
    <launcherErr>Error: Launcher Error</launcherErr>
  </messages>
</launch4jConfig>
